{
    "port": 3000,
    "corsEnabled": false,
    "corsOptions": {
        "origin": "*",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "OPTIONS"
        ],
        "allowedHeaders": [
            "Content-Type",
            "Authorization"
        ]
    },
    "auth": {
        "enabled": true,
        "jwtExpiration": "1h"
    },
    "git": {
        "allowedPrefixes": [
            "https://gitee.com/NekoGong/"
        ]
    }
}