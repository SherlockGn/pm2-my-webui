{
  "name": "pm2-webui",
  "version": "1.0.0",
  "description": "A web UI for PM2 process manager",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "pm2:start": "node run-as-pm2.js",
    "pm2:stop": "node run-as-pm2.js stop",
    "pm2:restart": "node run-as-pm2.js restart",
    "pm2:status": "node run-as-pm2.js status",
    "pm2:logs": "node run-as-pm2.js logs",
    "pm2:delete": "node run-as-pm2.js delete",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "pm2",
    "webui",
    "process-manager",
    "express",
    "vue"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
